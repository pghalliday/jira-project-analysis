{
  "name": "jira-project-analysis",
  "version": "0.1.0",
  "description": "Export JIRA project data as csv for analysis",
  "scripts": {
    "lint": "coffeelint -f coffeelint.json test/**.coffee lib/**.coffee",
    "unit-test": "mocha",
    "coverage": "multi='spec=- html-cov=coverage.html travis-cov=-' mocha --require coffee-coverage/register -R mocha-multi",
    "pretest": "npm run lint",
    "test": "npm run coverage"
  },
  "config": {
    "travis-cov": {
      "threshold": 100
    }
  },
  "keywords": [
    "jira",
    "csv",
    "time-series",
    "correlation"
  ],
  "homepage": "https://github.com/pghalliday/jira-project-analysis",
  "bugs": {
    "url": "https://github.com/pghalliday/jira-project-analysis/issues"
  },
  "license": "MIT",
  "author": {
    "name": "Peter Halliday",
    "email": "pghalliday@gmail.com",
    "url": "http://pghalliday.com"
  },
  "bin": "./bin/jira-project-analysis",
  "repository": {
    "type": "git",
    "url": "https://github.com/pghalliday/jira-project-analysis.git"
  },
  "dependencies": {
    "JSONStream": "^0.10.0",
    "coffee-script": "^1.8.0",
    "csv-stringify": "0.0.5",
    "moment": "^2.9.0",
    "progress": "^1.1.8",
    "q": "^1.1.2",
    "q-io": "^1.11.6",
    "request": "^2.51.0",
    "stream-reduce": "^1.0.3",
    "underscore": "^1.7.0"
  },
  "devDependencies": {
    "chai": "^1.10.0",
    "coffee-coverage": "^0.4.4",
    "coffeelint": "^1.8.1",
    "mocha": "^2.1.0",
    "mocha-multi": "^0.6.0",
    "sinon": "^1.12.2",
    "sinon-chai": "^2.6.0",
    "travis-cov": "^0.2.5"
  }
}
