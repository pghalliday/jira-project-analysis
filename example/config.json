{
  "output": "./jira-issues.csv",
  "project": [
    "MyProject"
  ],
  "exclude": {
    "types": [
      "Epic",
      "Requirement",
      "Test"
    ],
    "statuses": [
      "Parked",
      "Trash"
    ]
  },
  "statusMap": {
    "todo": [
      "To Do"
    ],
    "inProgress": [
      "In Progress",
      "Ready for Merging"
    ],
    "done": [
      "Closed",
      "Resolved",
      "Done"
    ],
    "ignore": [
      "Parked"
    ]
  },
  "jira": {
    "protocol": "https",
    "host": "jira.mydomain.com",
    "port": 443,
    "username": "username",
    "password": "password",
    "strictSSL": true
  }
}
