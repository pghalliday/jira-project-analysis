{
  "output": {
    "days": "./jira-days.csv",
    "issues": "./jira-issues.csv"
  },
  "days": 90,
  "project": [
    "MyProject"
  ],
  "exclude": {
    "types": [
      "Epic",
      "Requirement",
      "Test"
    ],
    "statuses": [
      "Parked",
      "Trash"
    ]
  },
  "statusMap": {
    "todo": [
      "To Do"
    ],
    "inProgress": [
      "In Progress",
      "Ready for Merging"
    ],
    "done": [
      "Closed",
      "Resolved",
      "Done"
    ]
  },
  "jira": {
    "protocol": "https",
    "host": "jira.mydomain.com",
    "port": 443,
    "username": "username",
    "password": "password",
    "apiVersion": "2",
    "verbose": false,
    "strictSSL": true,
    "oauth": false
  }
}
